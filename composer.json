{
    "name": "dazzlesoftware/cache",
    "type": "library",
    "description": "Dazzle Cache Package",
    "keywords": ["dazzle", "framework", "cache"],
    "homepage": "https://github.com/dazzle-framework/cache",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "psr/cache": "~1.0",
        "psr/simple-cache": "~1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "dazzlesoftware/test": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "ext-apcu": "To use APCu caching",
        "ext-memcached": "To use Memcached caching",
        "ext-redis": "To use Redis caching",
        "ext-wincache": "To use WinCache caching",
        "ext-xcache": "To use XCache caching"
    },
    "provide": {
        "psr/cache-implementation": "~1.0",
        "psr/simple-cache-implementation": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Cache\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
